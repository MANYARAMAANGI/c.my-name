// program to demonstrate usage of arithmetic operations
#include <stdio.h>
int main()
{
    int z;
    int y=20;
    int x=10;
    printf("z is %d\n",z);
    z=x+y;
    printf("z is %d\n",z);
    z=x-y;
    printf("z is %d\n",z);
    z=x*y;
    printf("z is %d\n",z);
    z=x/y;
    printf("z is %d\n",z);
    z=x%y;
    return 0;
  }  