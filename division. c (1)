// C Program To Check Whether a Number is Divisible by 5 or not 
#include <stdio.h>
int main()
{
    int num;
    // Asking for Input
    printf("Enter an number: ");
    scanf("%d", &num);
    
    if (num % 5 == 0)
    {
        printf("%d is divisible by 5 .", num);
    }
    else{
        printf("%d is not divisible by 5 and.", num);
    }
    return 0;
}